{
  "name": "dreams-of-darkness",
  "version": 2,
  "routes": [
    { "src": "/(.*)", "dest": "/build/$1"}
  ],
  "builds": [
    { "src": "build/**/*", "use": "@now/static" }
  ],
  "alias": [
    "dreamsofdarkness.com",
    "thedreamsofdarkness.com",
    "reddinghaunt.com",
    "www.dreamsofdarkness.com",
    "www.thedreamsofdarkness.com",
    "www.reddinghaunt.com"
  ]
}
